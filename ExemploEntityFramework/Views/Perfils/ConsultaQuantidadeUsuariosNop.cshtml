@model IEnumerable<ExemploEntityFramework.Models.UsuarioNopModel>

@{
    ViewBag.Title = "ConsultaQuantidadeUsuariosNop";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ConsultaQuantidadeUsuariosNop</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdNop)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuantidadeUsuarios)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdNop)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuantidadeUsuarios)
        </td>
    </tr>
}

</table>
